    <link href="~/css/prettyPhoto.css" rel="stylesheet" />
    <link href="~/css/price-range.css" rel="stylesheet" />
    <link href="~/css/responsive.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/assets/css/owl.carousel.css">
    <link rel="stylesheet" href="~/assets/css/owl.transitions.css">



    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <!--[if lt IE 9]>
    <script src="js/html5shiv.js"></script>
    <script src="js/respond.min.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="~/images/ico/favicon.ico" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/images/ico/apple-touch-icon-144-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/images/ico/apple-touch-icon-114-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/images/ico/apple-touch-icon-72-precomposed.png" />
    <link rel="apple-touch-icon-precomposed" href="~/images/ico/apple-touch-icon-57-precomposed.png" />
</head>
<body ng-app="MyApp">
    <header id="header" ng-controller="GetManuListController">
        <!--header-->
        <div class="header_top">
            <!--header_top-->
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="contactinfo">
                            <ul class="nav nav-pills">
                               
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="social-icons pull-right">
                            <ul class="nav navbar-nav">
                                <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                                <li><a href="#"><i class="fa fa-user"></i><span>@Session["Name"]</span></a></li>                               
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div><!--/header_top-->

        <div class="header-middle">
            <!--header-middle-->
            <div class="container">
                <div class="row">
                    <div class="col-md-4 clearfix"> 
                        <div class="logo pull-left">
                            
                            <a href="@Url.Action("Index","Home")"><img src="~/images/shop/sb.png" /></a>
                        </div>
                        <div class="btn-group pull-right clearfix">
                          
                        </div>
                    </div>
                    <div class="col-md-8 clearfix">

                        <div class="shop-menu clearfix pull-right">
                            
                            <ul class="nav navbar-nav">
                                <li><a href="#"><i class="fa fa-user"></i> Account</a></li>                                
                                <li><a href="#"><i class="fa fa-shopping-cart"></i> Cart</a></li>
                                <li data-toggle="modal" data-target=".bs-example-modal-sm"><i class="fa fa-lock"></i> Login</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!----------LoginModel----------->
            <div class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" style="display: none;" aria-hidden="true">
                <div class="modal-dialog modal-sm">
                    <div class="modal-content" style="color:black;">
                        <div class="modal-header" style="border-bottom:2px solid #ff6a00;background-color: #0c0c0c;">
                            <h4 class="modal-title text-center" style="color:#ffffff" id="mySmallModalLabel">LOGIN</h4>
                        </div>
                        <form class="form" novalidate name="f1" ng-submit="Login()">
                            <div style="position: relative; padding: 30px;">
                                <div class="row">
                                    <div class="form-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text bg-eureka bg-eureka "><i class="fa fa-user"></i></span>&nbsp;&nbsp;<label>Email Id / Mobile Number</label>
                                        </div>

                                        <input type="text" class="form-control" placeholder="Email Id / Mobile Number" name="UserId" autocomplete="off" ng-model="LoginData.UserId" ng-class="Submitted?'ng-dirty':''" required autofocus>
                                        <span class="error" style="color:red" ng-show="(f1.UserId.$dirty || Submitted) && f1.UserId.$error.required">Email Id / Mobile Number Required</span>
                                    </div>
                                    <div class="form-group">

                                        <div class="input-group-prepend">
                                            <span class="input-group-text bg-eureka  bg-eureka  "><i class="fa fa-lock"></i></span>&nbsp;&nbsp;<label>Password</label>
                                        </div>
                                        <input type="password" class="form-control" autocomplete="off" placeholder="Password" ng-model="LoginData.Password" name="Password" ng-class="Submitted?'ng-dirty':''" required autofocus>
                                        <span class="error" style="color:red" ng-show="(f1.Password.$dirty || Submitted) && f1.Password.$error.required">Password Required</span>
                                    </div>
                                </div>
                                <div class="row" style="padding-left: 195px;">
                                    <button type="submit" id="row-count" class="btn btn-xs btn-primary pull-right" style="border-radius:5px">LOGIN</button>
                                    <div style="cursor:pointer"><a href="@Url.Action("Register", "Home")"><label>Register</label></a> </div>
                                </div>
                               

                            </div>
                        </form>
                    </div>
                    <!-- /.modal-content -->
                </div>
            </div>

            <!-- /.modal------------END-------------------dialog -->




        </div><!--/header-middle-->


        <div class="header-nav animate-dropdown">
            <div class="container">
                <div class="yamm navbar navbar-default" role="navigation">
                    <div class="navbar-header">
                        <button data-target="#mc-horizontal-menu-collapse" data-toggle="collapse" class="navbar-toggle collapsed" type="button">
                            <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="nav-bg-class">
                        <div class="navbar-collapse collapse" id="mc-horizontal-menu-collapse">
                            <div class="nav-outer">
                                <ul class="nav navbar-nav">
                                    <li style="cursor:pointer;" class="dropdown mega-menu" ng-repeat="e in ManuData.CategoryList"><a ng-click="getCategoryID(e.CategoryID)">{{e.CategoryName}}</a></li>
                                </ul>
                                <!-- /.navbar-nav -->
                                <div class="clearfix"></div>
                            </div>
                            <!-- /.nav-outer -->
                        </div>
                        <!-- /.navbar-collapse -->

                    </div>
                    <!-- /.nav-bg-class -->
                </div>
                <!-- /.navbar-default -->
            </div>
            <!-- /.container-class -->

        </div>
        <!--/header-bottom-->
    </header><!--/header-->

    <section>
        @RenderBody()
    </section>
    <footer id="footer">
        <!--Footer-->


        <div class="footer-widget">
            <div class="container">
                <div class="row">
                    <div class="col-sm-2">
                        <div class="single-widget">
                            <h2>Service</h2>
                            <ul class="nav nav-pills nav-stacked">
                                <li><a href="#">Online Help</a></li>
                                <li><a href="#">Contact Us</a></li>
                              
                            </ul>
                        </div>
                    </div>
                    <div class="col-sm-2">
                        <div class="single-widget">
                            <h2>Quock Shop</h2>
                            <ul class="nav nav-pills nav-stacked">
                                <li><a href="#">Mat</a></li>
                                <li><a href="#">Wallhanging</a></li>
                                <li><a href="#">Collage</a></li>
                                <li><a href="#">Terracotta</a></li>
                               
                            </ul>
                        </div>
                    </div>
                  

                </div>
            </div>
        </div>

        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    @*<p class="pull-left">Copyright © 2013 E-SHOPPER Inc. All rights reserved.</p>*@
                    @*<%--<p class="pull-right">Designed by <span><a target="_blank" href="Login.aspx">Admin</a></span></p>--%>*@
                </div>
            </div>
        </div>
        @*<%--	http://www.themeum.com--%>*@
    </footer>

